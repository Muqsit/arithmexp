{
    "name": "muqsit/arithmexp",
    "description": "Mathematical expression parser and evaluator for PHP",
    "type": "library",
    "license": "GPL-3.0-only",
    "require": {
        "php": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "muqsit\\arithmexp\\": "src/muqsit/arithmexp"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "muqsit\\arithmexp\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Muqsit",
            "email": "notmuqsit@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^11"
    },
    "extra": {
        "virion": {
            "spec": "3.0",
            "namespace-root": "muqsit\\arithmexp"
        }
    }
}
